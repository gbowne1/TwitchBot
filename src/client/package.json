{
  "name": "twitchbot-client",
  "version": "1.0.0",
  "description": "the twitchbot client",
  "main": "index.js",
  "scripts": {
    "start": "cross-env ESLINT_NO_DEV_ERRORS=true; react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:quick": "quick-lint-js ./src",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier . --write",
    "preflight": "npm run lint && npm run prettier",
    "preflight:fix": "npm run lint:fix && npm run prettier:fix"
  },
  "keywords": [
    "twitch",
    "bot",
    "client"
  ],
  "author": "Greg Bowne",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.10.8",
    "@emotion/styled": "^11.10.8",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.12.3",
    "@tanstack/react-query": "^5.22.2",
    "axios": "^1.4.0",
    "chai": "^4.3.7",
    "chalk": "^5.2.0",
    "cheerio": "^1.0.0-rc.12",
    "discord.js": "^14.10.2",
    "ejs": "^3.1.9",
    "handlebars": "^4.7.7",
    "node-fetch": "^3.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.11.1",
    "react-router-dom": "^6.11.1",
    "react-scripts": "^5.0.1",
    "socket.io-client": "^4.6.1",
    "web-vitals": "^3.3.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-property-in-object": "^7.23.4",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.2.5",
    "quick-lint-js": "^3.1.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^axios$": "axios/dist/node/axios.cjs"
    }
  }
}
